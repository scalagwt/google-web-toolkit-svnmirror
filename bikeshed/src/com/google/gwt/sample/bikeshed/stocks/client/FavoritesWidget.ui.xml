<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:c='urn:import:com.google.gwt.user.cellview.client'>

  <ui:with field='styles'
    type='com.google.gwt.sample.bikeshed.style.client.Styles'/>

  <g:DockLayoutPanel unit='EM'>
    <g:north size='2'>
      <g:Label styleName='{styles.common.headerLeft}'>Portfolio / Favorites</g:Label>
    </g:north>

    <g:center>
      <g:ScrollPanel>
        <c:CellTable ui:field='table' styleName='{styles.common.table}'/>
      </g:ScrollPanel>
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder>

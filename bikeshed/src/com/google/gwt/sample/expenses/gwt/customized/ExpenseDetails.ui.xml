<!DOCTYPE ui:UiBinder SYSTEM 'http://dl.google.com/gwt/DTD/xhtml.ent'>
<ui:UiBinder
  xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:g='urn:import:com.google.gwt.user.client.ui'
  xmlns:l='urn:import:com.google.gwt.bikeshed.list.client'>

  <ui:style>
    .label {
      font-weight: bold;
    }
  </ui:style>

  <g:HTMLPanel
    width='100%'
    height='100%'>
    <h1>Jamaica Trip</h1>
    <table>
      <td
        class='{style.label}'>
        Total Approved:
      </td>
      <td>
        <g:Label
          ui:field='totalLabel'></g:Label>
      </td>
    </table>
    <span
      class='{style.label}'>Notes:</span>
    <g:TextBox
      ui:field='notesBox'></g:TextBox>
    <l:PagingTableListView
      width='100%'
      ui:field='listView' />
  </g:HTMLPanel>

</ui:UiBinder> 